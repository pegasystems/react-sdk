    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "sass-loader": "^16.0.5",
    "shx": "^0.4.0",
    "storybook": "^8.6.14",
    "style-loader": "^4.0.0",
    "stylelint": "^16.23.1",
    "tinymce": "^6.8.6",
    "trim-newlines": "^5.0.0",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "update-browserslist-db": "^1.0.13",
    "webpack": "^5.101.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "browser": {
    "[module-name]": false
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
